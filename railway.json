{
    "schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "cargo build --release --bin webhook"
    },
    "deploy": {
      "startCommand": "ls -la ./target/release && ./target/release/webhook",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }